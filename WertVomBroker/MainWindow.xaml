<Window x:Class="WertVomBroker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="28" Margin="35,16,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="136"/>
        <Canvas x:Name="mycanvas" HorizontalAlignment="Left" Height="315" Margin="35,63,0,0" VerticalAlignment="Top" Width="500"/>
        <Button x:Name="buttonAus" Content="Lüfter aus" HorizontalAlignment="Left" Margin="208,21,0,0" VerticalAlignment="Top" Width="75" Click="ButtonAus_Click"/>
        <Button x:Name="buttonEin" Content="Lüfter ein" HorizontalAlignment="Left" Margin="343,21,0,0" VerticalAlignment="Top" Width="75" Click="ButtonEin_Click"/>
        <lvc:CartesianChart x:Name="chart" Margin="35,63,38,10" />
    </Grid>
</Window>